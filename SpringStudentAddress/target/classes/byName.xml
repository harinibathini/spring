<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="address" class="com.bean.Address">
        <property name="street" value="street"></property>
        <property name="city" value="city"></property>
        <property name="state" value="state"></property>
        <property name="zipcode" value="112343"></property>
    </bean>
    <bean id="student" class="com.bean.Student" autowire="byName">
        <property name="name" value="Shreya"></property>
        <property name="age" value="22"></property>
    </bean>

